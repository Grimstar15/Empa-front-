,SAAS,EAAEN,OAAO,CAACO;AAAxB;AAAA;AAAA;AAAA;AAAA,cADJ,eAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAFJ;AAAA;AAAA;AAAA;AAAA;AAAA,YADJ,eAKI;AAAK,MAAA,SAAS,EAAEP,OAAO,CAACM,IAAxB;AAAA,8BACI;AAAK,QAAA,SAAS,EAAEN,OAAO,CAACO;AAAxB;AAAA;AAAA;AAAA;AAAA,cADJ,eAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAFJ;AAAA;AAAA;AAAA;AAAA;AAAA,YALJ,eASI;AAAK,MAAA,SAAS,EAAEP,OAAO,CAACM,IAAxB;AAAA,8BACI;AAAK,QAAA,SAAS,EAAEN,OAAO,CAACO;AAAxB;AAAA;AAAA;AAAA;AAAA,cADJ,eAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAFJ;AAAA;AAAA;AAAA;AAAA;AAAA,YATJ,eAaI;AAAK,MAAA,SAAS,EAAEP,OAAO,CAACM,IAAxB;AAAA,8BACI;AAAK,QAAA,SAAS,EAAEN,OAAO,CAACO;AAAxB;AAAA;AAAA;AAAA;AAAA,cADJ,eAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAFJ;AAAA;AAAA;AAAA;AAAA;AAAA,YAbJ,eAiBI;AAAK,MAAA,SAAS,EAAEP,OAAO,CAACM,IAAxB;AAAA,8BACI;AAAK,QAAA,SAAS,EAAEN,OAAO,CAACO;AAAxB;AAAA;AAAA;AAAA;AAAA,cADJ,eAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAFJ;AAAA;AAAA;AAAA;AAAA;AAAA,YAjBJ;AAAA;AAAA;AAAA;AAAA;AAAA,UANJ;AAAA;AAAA;AAAA;AAAA;AAAA,QADJ;;AAgCA,eAAeN,MAAf","sourcesContent":["import React from 'react';\r\n\r\nimport classes from './KidsBenefits.module.scss';\r\n\r\nconst values = (props) => (\r\n    <div className={classes.Container}>\r\n        <div className={classes.HeaderContainer}>\r\n            <h2>Та хүүхдийнхээ цахим хэрэглээг хянаж чадаж байна уу?</h2>\r\n            <p>Интернет хүүхдэд тийм ч ээлтэй газар биш. Ялангуяа ямар нэг хамгаалалтгүй үед. Таны хүүхэд өөрийн дуртай веб сайт руугаа орох гэж байхад насанд хүрэгчдийн сайт руу автоматаар орох, садар самуун болон хүчирхийллийг сурталчилсан реклам гарахыг үгүйсгэхгүй. Иймд эцэг эхчүүд тоглоомын талбайд, анги танхимд нь хүүхдийнхээ аюулгүй байдалд анхаардаг шигээ интернет орчинд ч өөрсдийн хяналтыг тавих хэрэгтэй. Хүүхдийнхээ сэтгэл зүйн аюулгүй байдлыг хамгаалалтгүй компьютерт даатгаж болохгүй.</p>\r\n            <p>Kaspersky Safe Kids нь хүүхдийн олон нийтийн сүлжээн дэх хэрэглээг хянан тэднийг хамгаалах зорилгоор бүтээгдсэн ба хэрэглэж байгаа аппликейшны тохиргоог та өөрийн компьютер, гар утаснаасаа удирдаж, хэрэглээг хянах зориулалттай.</p>\r\n        </div>\r\n        <div className={classes.ContentContainer}>\r\n            <div className={classes.Item}>\r\n                <div className={classes.Circle}></div>\r\n                <p>Хүүхдийн байршил харах, утасны цэнэг харах</p>\r\n            </div>\r\n            <div className={classes.Item}>\r\n                <div className={classes.Circle}></div>\r\n                <p>Утас болон аппликейшныг хэдэн минут ашигласан мэдээлэл харах</p>\r\n            </div>\r\n            <div className={classes.Item}>\r\n                <div className={classes.Circle}></div>\r\n                <p>Тоглоом болон хэрэгцээгүй аппликейшн руу нэвтрэх эрхийг хязгаарлах</p>\r\n            </div>\r\n            <div className={classes.Item}>\r\n                <div className={classes.Circle}></div>\r\n                <p>Тухайн аппликейшныг өдөрт хэдэн цаг ашиглах хугацааг тохируулах</p>\r\n            </div>\r\n            <div className={classes.Item}>\r\n                <div className={classes.Circle}></div>\r\n                <p>Олон нийтийн сүлжээнд хүүхдийн оруулж буй мэдээлэл, бичлэг болон шинээр нэмэгдсэн найз гэх мэт мэдээллийг харах</p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n);\r\n\r\nexport default values;"]},"metadata":{},"sourceType":"module"}
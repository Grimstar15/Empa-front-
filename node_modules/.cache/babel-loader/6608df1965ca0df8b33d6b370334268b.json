\u044D\\u0446\\u0441\\u0438\\u0439\\u043D \\u0446\\u044D\\u0433\\u0438\\u0439\\u043D \\u0431\\u04AF\\u0442\\u044D\\u044D\\u0433\\u0434\\u044D\\u0445\\u04AF\\u04AF\\u043D\\u0438\\u0439\\u0433 \\u0441\\u043E\\u043D\\u0433\\u043E\\u043E\\u0440\\u043E\\u0439.\"})]}),/*#__PURE__*/_jsxs(\"div\",{className:classes.item,children:[/*#__PURE__*/_jsx(\"img\",{src:\"http://empasoft.mn/wp-content/uploads/2020/03/e4.png\"}),/*#__PURE__*/_jsx(\"h2\",{children:\"\\u0422\\u043E\\u043C\\u043E\\u043E\\u0445\\u043E\\u043D \\u0431\\u0438\\u0437\\u043D\\u0435\\u0441 \\u044D\\u0440\\u0445\\u043B\\u044D\\u0433\\u0447\\u0434\\u044D\\u0434 (1000 \\u0434\\u044D\\u044D\\u0448 \\u0445\\u044D\\u0440\\u044D\\u0433\\u043B\\u044D\\u0433\\u0447)\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0410\\u043B\\u0438\\u0432\\u0430\\u0430 \\u043A\\u0438\\u0431\\u0435\\u0440 \\u0430\\u044E\\u0443\\u043B\\u044B\\u0433 \\u0442\\u0430\\u0430\\u043C\\u0430\\u0433\\u043B\\u0430\\u0436, \\u0445\\u0430\\u043C\\u0433\\u0430\\u0430\\u043B\\u0436, \\u0445\\u0430\\u0440\\u0438\\u0443 \\u04AF\\u0439\\u043B\\u0434\\u044D\\u043B \\u04AF\\u0437\\u04AF\\u04AF\\u043B\\u044D\\u0445 \\u0445\\u044D\\u043C\\u0436\\u044D\\u044D\\u043D\\u0438\\u0439 \\u0448\\u0438\\u0439\\u0434\\u043B\\u0438\\u0439\\u0433 Kaspersky \\u0442\\u043E\\u043C\\u043E\\u043E\\u0445\\u043E\\u043D \\u0433\\u0440\\u0443\\u043F\\u043F \\u043A\\u043E\\u0440\\u043F\\u043E\\u0440\\u0430\\u0446\\u0438\\u0443\\u0434\\u0430\\u0434 \\u0441\\u0430\\u043D\\u0430\\u043B \\u0431\\u043E\\u043B\\u0433\\u043E\\u0436 \\u0431\\u0430\\u0439\\u043D\\u0430. \\u0414\\u044D\\u043B\\u0445\\u0438\\u0439\\u043D \\u043E\\u043B\\u043E\\u043D \\u0441\\u0430\\u044F \\u0445\\u044D\\u0440\\u044D\\u0433\\u043B\\u044D\\u0433\\u0447\\u0438\\u0439\\u043D \\u043C\\u0430\\u0430\\u043D\\u044C \\u0442\\u043E\\u0434\\u043E\\u0440\\u0445\\u043E\\u0439 \\u0445\\u0443\\u0432\\u044C \\u044D\\u043D\\u044D \\u0442\\u04E9\\u0440\\u043B\\u0438\\u0439\\u043D \\u0431\\u04AF\\u0442\\u044D\\u044D\\u0433\\u0434\\u044D\\u0445\\u04AF\\u04AF\\u043D\\u0438\\u0439\\u0433 \\u0445\\u044D\\u0440\\u044D\\u0433\\u043B\\u044D\\u0436 \\u0431\\u0430\\u0439\\u0433\\u0430\\u0430\\u0434\\u0430\\u0430 \\u0441\\u044D\\u0442\\u0433\\u044D\\u043B \\u0445\\u0430\\u043D\\u0433\\u0430\\u043B\\u0443\\u0443\\u043D \\u0431\\u0430\\u0439\\u043D\\u0430.\"})]})]})]});}","map":{"version":3,"sources":["/home/empawebadmin/empaweb/frontend/src/components/ProductList/Products/kaspersky/index.js"],"names":["React","classes","Kaspersky","Container","Items","item"],"mappings":"AAAA,MAAOA,CAAAA,KAAP,KAAkB,OAAlB,CACA,MAAOC,CAAAA,OAAP,KAAoB,yBAApB,C,wFACA,cAAe,SAASC,CAAAA,SAAT,EAAqB,CAClC,mBACE,oCACE,aAAK,SAAS,CAAED,OAAO,CAACE,SAAxB,wBACE,mFADF,cAEE,2jDAKoD,GALpD,GAFF,cASE,mCACE,4CADF,cAEE,mDAFF,cAGE,gDAHF,GATF,GADF,cAgBE,aAAK,SAAS,CAAEF,OAAO,CAACG,KAAxB,wBACE,aAAK,SAAS,CAAEH,OAAO,CAACI,IAAxB,wBACE,YAAK,GAAG,CAAC,sDAAT,EADF,cAEE,gHAFF,cAGE,owCAIsD,GAJtD,GAHF,GADF,cAWE,aAAK,SAAS,CAAEJ,OAAO,CAACI,IAAxB,wBACE,YAAK,GAAG,CAAC,sDAAT,EADF,cAEE,sNAFF,cAGE,2wCAHF,GAXF,cAqBE,aAAK,SAAS,CAAEJ,OAAO,CAACI,IAAxB,wBACE,YAAK,GAAG,CAAC,sDAAT,EADF,cAEE,kNAFF,cAGE,gwCAHF,GArBF,cA+BE,aAAK,SAAS,CAAEJ,OAAO,CAACI,IAAxB,wBACE,YAAK,GAAG,CAAC,sDAAT,EADF,cAEE,iQAFF,cAGE,m1CAHF,GA/BF,GAhBF,GADF,CA6DD","sourcesContent":["import React from \"react\";\r\nimport classes from \"./kaspersky.module.scss\";\r\nexport default function Kaspersky() {\r\n  return (\r\n    <div>\r\n      <div className={classes.Container}>\r\n        <h2>Kaspersky Lab гэж юу вэ?</h2>\r\n        <p>\r\n          Kaspersky Lab нь 1997 онд байгуулагдсан дэлхийн кибер аюулгуй байдлын\r\n          компани юм. Дижитал аюул заналтай тэмцэх төгсгөлийн цэгийг хамгаалах\r\n          бүтээгдэхүүн нь мэргэжлийн бөгөөд аюулгүй байдлын хамгийн тохиромжтой\r\n          шийдэл юм. Өдгөө дэлхий дахинаа 400 гаран сая хэрэглэгч, 270 мянган\r\n          компани Kaspersky бүтээгдэхүүнийг хэрэглэж байна.{\" \"}\r\n        </p>\r\n        <ul>\r\n          <li>Kaspersky Anti-Virus</li>\r\n          <li>Kaspersky Internet Security</li>\r\n          <li>Kaspersky Total Security</li>\r\n        </ul>\r\n      </div>\r\n      <div className={classes.Items}>\r\n        <div className={classes.item}>\r\n          <img src=\"http://empasoft.mn/wp-content/uploads/2020/03/e4.png\" />\r\n          <h2>Гэр бүр, хувь хүнд</h2>\r\n          <p>\r\n            Дижитал ертөнцөд компьютер, таблет, гар утас зэрэг төхөөрөмжөөр таны\r\n            сошиал хэрэглээ, онлайн худалдаа, онлайн банкны мэдээллийг хамгаалах\r\n            үүрэгтэй. Kaspersky Safe Kids бүтээгдэхүүн таны хүүхдийг интернет\r\n            орчинд хамгаалалж, цахим хэрэглээн хянах боломжтой.{\" \"}\r\n          </p>\r\n        </div>\r\n        <div className={classes.item}>\r\n          <img src=\"http://empasoft.mn/wp-content/uploads/2020/03/e4.png\" />\r\n          <h2>Жижиг бизнес эрхлэгчдэд (1-50 хэрэглэгч)</h2>\r\n          <p>\r\n            Жижиг бизнес эрхэлдэг таны цаг хугацаа таны бизнестэй адил үнэтэй\r\n            байдаг. Kaspersky бүтээгдэхүүн таны бизнесийг кибер аюулаас\r\n            сэргийлэлэх болно. Мэдээлэл технологийн ур чадварын түвшнээс үл\r\n            хамааран таны бизнест хамаатай олон зүйлийг бид хамгаална.\r\n          </p>\r\n        </div>\r\n        <div className={classes.item}>\r\n          <img src=\"http://empasoft.mn/wp-content/uploads/2020/03/e4.png\" />\r\n          <h2>Дунд бизнес эрхлэгчдэд (51-999 хэрэглэгч)</h2>\r\n          <p>\r\n            Хэрэв таны бизнес хурдацтай хөгжиж, дижитал өөрчлөлтийн эрчимтэй\r\n            хийж эхэлсэн бол төдий чинээ кибер аюулгүй байдлаа ханган ажиллах\r\n            хэрэгтэй болно. Тиймээс та таны үнэт мэдээллийг шуурхай хамгаалах\r\n            эцсийн цэгийн бүтээгдэхүүнийг сонгоорой.\r\n          </p>\r\n        </div>\r\n        <div className={classes.item}>\r\n          <img src=\"http://empasoft.mn/wp-content/uploads/2020/03/e4.png\" />\r\n          <h2>Томоохон бизнес эрхлэгчдэд (1000 дээш хэрэглэгч)</h2>\r\n          <p>\r\n            Аливаа кибер аюулыг таамаглаж, хамгаалж, хариу үйлдэл үзүүлэх\r\n            хэмжээний шийдлийг Kaspersky томоохон групп корпорациудад санал\r\n            болгож байна. Дэлхийн олон сая хэрэглэгчийн маань тодорхой хувь энэ\r\n            төрлийн бүтээгдэхүүнийг хэрэглэж байгаадаа сэтгэл хангалуун байна.\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n"]},"metadata":{},"sourceType":"module"}